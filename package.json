    "@types/crypto-js": "4.2.1",
    "@types/jsonwebtoken": "9.0.5",
    "@types/nodemailer": "6.4.14",
    "@types/uuid": "9.0.7",
    "cross-env": "7.0.3",
    "dotenv": "16.4.1",
    "eslint": "8.55.0",
    "home": "2.0.0",
    "node-ssh": "13.1.0",
    "nuxt": "^3.8.2",
    "sass": "1.69.5",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
