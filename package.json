{
  "name": "idea-press",
  "version": "0.0.0",
  "main": "server.js",
  "repository": "https://github.com/Yakima-Teng/IdeaPress.git",
  "author": "Yakima <cleveryun@163.com>",
  "private": false,
  "license": "MIT",
  "scripts": {
    "eslint": "eslint --fix --format 'node_modules/eslint-friendly-formatter' --ext .js,.jsx src",
    "dev": "nodemon",
    "devProduction": "next build src && cross-env NODE_ENV=production ts-node --project tsconfig.json server.ts",
    "start": "next build src && pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop IdeaPress",
    "restart": "next build src && pm2 restart IdeaPress",
    "delete": "pm2 delete IdeaPress",
    "getShellUsedToStartProjectAfterReboot": "pm2 startup",
    "log": "pm2 log",
    "status": "pm2 status"
  },
  "dependencies": {
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.0.3",
    "async": "^3.2.2",
    "babel-plugin-import": "^1.13.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "isomorphic-fetch": "^2.2.1",
    "less": "^3.11.1",
    "less-vars-to-js": "^1.3.0",
    "markdown-it": "^10.0.0",
    "md5-hex": "^3.0.1",
    "mysql": "^2.18.1",
    "next": "^9.3.0",
    "null-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "qs": "^6.8.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-markdown": "^4.3.1",
    "trim-html": "^0.1.9"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
